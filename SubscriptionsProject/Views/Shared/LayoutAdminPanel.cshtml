<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - Abonelikler Uygulaması</title>
	<meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>

	<link href="~/Content/others/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />

	<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />


	<link href="~/Content/others/dist/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />
	<link href="~/Content/others/dist/css/skins/_all-skins.min.css" rel="stylesheet" type="text/css" />
	<link href="~/Content/others/plugins/iCheck/flat/blue.css" rel="stylesheet" type="text/css" />
	<link href="~/Content/others/plugins/morris/morris.css" rel="stylesheet" type="text/css" />
	<link href="~/Content/others/plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" type="text/css" />
	<link href="~/Content/others/plugins/datepicker/datepicker3.css" rel="stylesheet" type="text/css" />
	<link href="~/Content/others/plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />
	<link href="~/Content/others/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" type="text/css" />


	<link href="~/Content/others/plugins/datatables/dataTables.bootstrap.css" rel="stylesheet" type="text/css" />

</head>
<body class="skin-blue">
	<div class="wrapper">

		<header class="main-header">
			<a href="../Admin/AdminPanel" class="logo"><b>FunLab </b>Panel</a>
			<nav class="navbar navbar-static-top" role="navigation">
				<a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
					<span class="sr-only">Toggle navigation</span>
				</a>
			</nav>
		</header>
		<aside class="main-sidebar">
			<section class="sidebar">


				<ul class="sidebar-menu">
					<li class="header">Menü</li>
					<li><a href="../Admin/AdminPanel"><i class="fa fa-book"></i> Ana Sayfa</a></li>
					<li class="active treeview">
						<a href="#">
							<i class="fa fa-user"></i> <span>Abonelik İşlemleri</span> <i class="fa fa-angle-left pull-right"></i>
						</a>
						<ul class="treeview-menu">
							<li class="active"><a href="../Admin/Users"><i class="fa fa-circle-o"></i>Kullanıcılar</a></li>
							<li class="active"><a href="../Admin/PaymentProcess"><i class="fa fa-circle-o"></i>Ödeme Takip</a></li>
						</ul>
					</li>
					<li>
						<br />	<br />	<br />	<br />	<br />	<br />
					</li>
					<li>
						<button class="btn btn-danger center-block" onclick="Logout()">Çıkış Yap</button>
					</li>
				</ul>
			</section>

		</aside>


		<div class="content-wrapper">


			<section class="content">

				@RenderBody()

			</section>
		</div>

	</div>

	<script src="~/Content/others/plugins/jQuery/jQuery-2.1.3.min.js"></script>



	<script src="~/Content/others/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/morris/morris.min.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/sparkline/jquery.sparkline.min.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js" type="text/javascript"></script>
	<script src="~/Content/others/plugins/jvectormap/jquery-jvectormap-world-mill-en.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/knob/jquery.knob.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/daterangepicker/daterangepicker.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/datepicker/bootstrap-datepicker.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/iCheck/icheck.min.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>

	<script src='~/Content/others/plugins/fastclick/fastclick.min.js'></script>

	<script src="~/Content/others/dist/js/app.min.js" type="text/javascript"></script>

	<script src="~/Content/others/dist/js/pages/dashboard.js" type="text/javascript"></script>

	<script src="~/Content/others/dist/js/demo.js" type="text/javascript"></script>

	<script src="~/Content/others/plugins/datatables/jquery.dataTables.js" type="text/javascript"></script>
	<script src="~/Content/others/plugins/datatables/dataTables.bootstrap.js" type="text/javascript"></script>

	<script type="text/javascript">

		function Logout() {

			$.ajax({
				url: '/Home/Logout',
				type: 'POST',
				success: function (result) {
					if (!result.success) {
						location.reload();

					} else {
						location.reload();
					}
				},
				error: function (xhr, status, error) {
					console.log(status + ': ' + error);
				}
			});

		}

	</script>
</body>

</html>
